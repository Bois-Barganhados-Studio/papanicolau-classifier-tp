<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import javafx.scene.layout.*?>
<?import javafx.collections.*?>
<?import javafx.scene.control.Button?>
<!-- Import string-->
<?import javafx.scene.chart.CategoryAxis?>
<?import java.lang.String?>

<?scenebuilder-stylesheet JMetroDarkTheme.css?>

<AnchorPane xmlns="http://javafx.com/javafx"
    xmlns:fx="http://javafx.com/fxml"
    fx:controller="com.boisbarganhados.view.HistogramController" styleClass="background">
    <children>
        <!-- Histogram chart -->
        <BarChart fx:id="barChart" title="Histogram Chart">
            <xAxis>
                <CategoryAxis fx:id="xAxix" side="BOTTOM"/>
              </xAxis>
              <yAxis>
                <NumberAxis fx:id="yAxix" side="LEFT"/>
              </yAxis>
        </BarChart>
        <HBox spacing="10" alignment="center">
            <Button fx:id="backButton" text="Voltar" />
        </HBox>
    </children>
</AnchorPane>